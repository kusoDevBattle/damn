{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [],
  "parserOptions": {},
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "Vue": false,
    "Event": false,
    "document": false,
    "window": false,
    "console": false
  },
  "rules": {
    "semi": ["error", "always"],
    "no-var": "error",
    "no-extra-semi": "warn"
  }
}
